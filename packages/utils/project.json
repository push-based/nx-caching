{
  "name": "utils",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc",
        "cwd": "packages/utils"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "report": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node src/generate-report.js",
        "cwd": "packages/utils"
      },
      "outputs": ["{projectRoot}/dist/report.txt"],
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test",
        "cwd": "packages/utils"
      }
    }
  }
}
